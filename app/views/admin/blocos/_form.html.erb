<% form_for @bloco do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :titulo %><br />
    <%= f.text_field :titulo %>
  </p>
  <p>
    <%= f.label :identificador %><br />
    <%= f.text_field :identificador %>
  </p>
  <p>
    <%= f.label :conteudo %><br />
    <%= f.text_area :conteudo %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
